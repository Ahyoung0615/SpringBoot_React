<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.brs.sun.model.dao.JsTreeDao">

	<select id="playerTable" resultType="PlayerVo">
		SELECT *
		FROM PLAYER p 
	</select>
	
	<select id="teamTable" resultType="TeamVo">
		SELECT *
		FROM TEAM t 
	</select>
	
	<select id="stadiumTable" resultType="StadiumVo">
		SELECT *
		FROM STADIUM s
	</select>
</mapper>
